generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum authority {
  ADMIN
  USER
}

enum kup {
  one
  two 
  three
  four
  five
  six
  seven
  eight
  nine
  ten
}

enum weight_class {
  to56kg
}

enum gender {
  MALE
  FEMALE
}

enum ag {
  Senior
  Youth_A 
  Youth_B
  Youth_C
  Youth_D
}

enum pg {
  KADETS
  LK1
  LK2
}

model User {
  id            String @id @default(cuid())
  username      String?
  password      String
  name          String
  surename      String?
  authority     authority @default(USER)
  failed_logins Int 
  email         String @unique
  birthday      DateTime?
  img           String?
  kup           kup?
  weight_class  weight_class?
  gender        gender?
  ag            ag?
  pg            pg?
  pushSubscription String?
  timestamp     DateTime
}
